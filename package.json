{
  "name": "graphql-auth",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsnd --no-notify --respawn --transpile-only ./src",
    "build": "tsc",
    "test": "dotenv -e .env.test -- jest",
    "start": "node dist/index.js",
    "debug": "tsnd --inspect --respawn --transpile-only ./src",
    "prebuild": "run-s clean generate:*",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "clean": "rimraf dist",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only ./src/schema",
    "db:save": "yarn prisma migrate dev --create-only --preview-feature",
    "db:migrate": "yarn prisma migrate dev --preview-feature",
    "db:migrate:prod": "yarn prisma migrate deploy --preview-feature",
    "postinstall": "yarn generate:prisma"
  },
  "dependencies": {
    "@prisma/client": "3.15.2",
    "apollo-server": "2.25.4",
    "bcrypt": "5.0.1",
    "dotenv": "16.0.1",
    "graphql": "15.8.0",
    "graphql-middleware": "6.1.31",
    "graphql-shield": "7.5.0",
    "jsonwebtoken": "8.5.1",
    "nexus": "1.3.0",
    "prisma": "3.15.2"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.0",
    "@types/dotenv": "8.2.0",
    "@types/eslint": "8.4.5",
    "@types/jest": "28.1.4",
    "@types/jsonwebtoken": "8.5.8",
    "@types/node": "16.11.43",
    "@types/prettier": "2.6.3",
    "@types/rimraf": "3.0.2",
    "@typescript-eslint/eslint-plugin": "5.30.6",
    "@typescript-eslint/parser": "5.30.6",
    "cross-env": "7.0.3",
    "dotenv-cli": "6.0.0",
    "eslint": "8.19.0",
    "graphql-request": "3.4.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "jest-environment-node": "27.5.1",
    "lint-staged": "12.5.0",
    "nanoid": "3.3.4",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "ts-jest": "27.1.5",
    "ts-node": "10.8.2",
    "ts-node-dev": "2.0.0",
    "typescript": "4.7.4",
    "typesync": "0.9.2"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "lint-staged": {
    "*.{js,ts}": "npm run lint",
    "*.{ts,js,json,md}": "prettier --write"
  }
}
