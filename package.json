{
  "name": "graphql-auth",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsnd --no-notify --respawn --transpile-only ./src",
    "build": "tsc",
    "test": "dotenv -e .env.test -- jest",
    "start": "node dist/index.js",
    "debug": "tsnd --inspect --respawn --transpile-only ./src",
    "prebuild": "run-s clean generate:*",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "clean": "rimraf dist",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only ./src/schema",
    "db:save": "yarn prisma migrate dev --create-only --preview-feature",
    "db:migrate": "yarn prisma migrate dev --preview-feature",
    "db:migrate:prod": "yarn prisma migrate deploy --preview-feature",
    "postinstall": "yarn generate:prisma"
  },
  "dependencies": {
    "@prisma/client": "3.15.2",
    "apollo-server": "2.26.1",
    "bcrypt": "5.1.0",
    "dotenv": "16.3.1",
    "graphql": "15.8.0",
    "graphql-middleware": "6.1.35",
    "graphql-shield": "7.6.5",
    "jsonwebtoken": "9.0.1",
    "nexus": "1.3.0",
    "prisma": "3.15.2"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.0",
    "@types/dotenv": "8.2.0",
    "@types/eslint": "8.4.10",
    "@types/jest": "28.1.8",
    "@types/jsonwebtoken": "8.5.9",
    "@types/node": "16.18.11",
    "@types/prettier": "2.7.2",
    "@types/rimraf": "3.0.2",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@typescript-eslint/parser": "5.60.1",
    "cross-env": "7.0.3",
    "dotenv-cli": "7.2.1",
    "eslint": "8.44.0",
    "graphql-request": "3.4.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "jest-environment-node": "27.5.1",
    "lint-staged": "12.5.0",
    "nanoid": "3.3.6",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.8",
    "rimraf": "3.0.2",
    "ts-jest": "27.1.5",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "typescript": "5.0.4",
    "typesync": "0.9.2"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "lint-staged": {
    "*.{js,ts}": "npm run lint",
    "*.{ts,js,json,md}": "prettier --write"
  }
}
