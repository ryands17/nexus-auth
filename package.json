{
  "name": "graphql-auth",
  "license": "MIT",
  "scripts": {
    "postinstall": "typesync",
    "predev": "npm run generate:prisma",
    "dev": "cross-env NODE_ENV=development tsnd --no-notify --respawn --transpileOnly ./src",
    "prebuild": "cross-env NODE_ENV=production npm run -s clean && npm run -s generate:prisma && npm run -s generate:nexus",
    "build": "cross-env NODE_ENV=production tsc",
    "seed": "ts-node prisma/seed.ts",
    "clean": "rimraf dist",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only ./src/schema",
    "db:save": "prisma migrate save --experimental --name",
    "db:migrate": "prisma migrate up --experimental",
    "studio": "prisma studio --experimental",
    "start": "cross-env NODE_ENV=production node dist/src/index.js"
  },
  "dependencies": {
    "@nexus/schema": "^0.13.1",
    "@prisma/client": "^2.0.0-beta.1",
    "apollo-server": "^2.11.0",
    "bcrypt": "4.0.1",
    "dotenv": "^8.1.0",
    "graphql-shield": "7.2.4",
    "graphql-yoga": "1.18.3",
    "helmet": "^3.22.0",
    "jsonwebtoken": "8.5.1",
    "nexus": "0.12.0",
    "nexus-prisma": "^0.12.0"
  },
  "devDependencies": {
    "@prisma/cli": "^2.0.0-beta.1",
    "@types/bcrypt": "3.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/eslint": "^6.1.9",
    "@types/helmet": "^0.0.45",
    "@types/jsonwebtoken": "8.3.8",
    "@types/node": "^13.11.0",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.10.1",
    "rimraf": "^3.0.0",
    "ts-node": "^8.8.2",
    "ts-node-dev": "1.0.0-pre.44",
    "typescript": "3.8.3",
    "typesync": "^0.6.1"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
