{
  "name": "graphql-auth",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsnd --no-notify --respawn --transpile-only ./src",
    "build": "tsc",
    "test": "dotenv -e .env.test -- jest",
    "start": "node dist/index.js",
    "debug": "tsnd --inspect --respawn --transpile-only ./src",
    "prebuild": "run-s clean generate:*",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "clean": "rimraf dist",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only ./src/schema",
    "db:save": "yarn prisma migrate dev --create-only --preview-feature",
    "db:migrate": "yarn prisma migrate dev --preview-feature",
    "db:migrate:prod": "yarn prisma migrate deploy --preview-feature",
    "postinstall": "yarn generate:prisma"
  },
  "dependencies": {
    "@prisma/client": "3.12.0",
    "apollo-server": "2.25.3",
    "bcrypt": "5.0.1",
    "dotenv": "16.0.0",
    "graphql": "15.8.0",
    "graphql-middleware": "6.1.24",
    "graphql-shield": "7.5.0",
    "jsonwebtoken": "8.5.1",
    "nexus": "1.3.0",
    "prisma": "3.12.0"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.0",
    "@types/dotenv": "8.2.0",
    "@types/eslint": "8.4.1",
    "@types/jest": "27.4.1",
    "@types/jsonwebtoken": "8.5.8",
    "@types/node": "16.11.27",
    "@types/prettier": "2.6.0",
    "@types/rimraf": "3.0.2",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "cross-env": "7.0.3",
    "dotenv-cli": "5.1.0",
    "eslint": "8.13.0",
    "graphql-request": "3.4.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "jest-environment-node": "27.5.1",
    "lint-staged": "12.3.8",
    "nanoid": "3.3.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.6.2",
    "rimraf": "3.0.2",
    "ts-jest": "27.1.4",
    "ts-node": "10.7.0",
    "ts-node-dev": "1.1.8",
    "typescript": "4.6.3",
    "typesync": "0.9.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "lint-staged": {
    "*.{js,ts}": "npm run lint",
    "*.{ts,js,json,md}": "prettier --write"
  }
}
